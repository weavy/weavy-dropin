@model IEnumerable<Blob>
@foreach (var blob in Model) {
  <div class="wy-item">
    <icon for="blob" />
    <div class="wy-item-body">@blob.Name</div>
    <button type="button" class="wy-button wy-button-icon" data-action="attachments#remove"><icon name="close-circle" /></button>
    <input type="hidden" name="blobs" value="@blob.Id" />
  </div>
}


