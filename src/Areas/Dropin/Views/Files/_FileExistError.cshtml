@model Blob

<div class="wy-dropdown-item wy-upload-error" data-files-target="uploadError">
  <span class="wy-upload-error-title">@Model.Name</span>
  <div class="wy-upload-error-text">
    @T["A file with that name already exists. Would you like to overwrite the file and create a new version?"]
  </div>
  <div class="wy-upload-error-actions">
    <button class="wy-button wy-button-primary" data-action="click->files#skip">@T["Skip"]</button>
    <button class="wy-button wy-button-primary" data-file data-action="click->files#replace" data-files-id-param="@Model.Id" data-files-uuid-param="@ViewData["uuid"]">@T["Replace"]</button></div>
</div>

